<div class="container">


<h1><%= @visit.title %></h1>

<p><%= @visit.body %></p>
<% if current_user && current_user.is_admin? %>
  <ul>
    <li><%= link_to "Edit", edit_visit_path(@visit) %></li>
    <li><%= link_to 'Destroy', @visit, method: :delete, data: { confirm: 'Are you sure?' } %></li>
  </ul>

<% end %>
</div>